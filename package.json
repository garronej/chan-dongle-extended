  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "tsc": "./node_modules/.bin/tsc -p ./tsconfig.json && chmod +x ./dist/bin/*.js",
    "tsc-w": "node ./node_modules/typescript/bin/tsc -w -p ./tsconfig.json",
    "start": "/bin/bash ./src/bin/start.sh"
