{
  "name": "chan-dongle-extended",
  "version": "1.4.0",
  "description": "feature SMS multipart, phonebook memory access, PIN code management",
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "tsc": "./node_modules/.bin/tsc -p ./tsconfig.json && chmod +x ./dist/bin/*.js",
    "postinstall": "if [ $(id -u) -eq 0  ]; then (node ./dist/bin/scripts postinstall); else (sudo node ./dist/bin/scripts postinstall); fi",
    "preuninstall": "if [ $(id -u) -eq 0  ]; then (node ./dist/bin/scripts preuninstall); else (sudo node ./dist/bin/scripts preuninstall); fi",
    "start": "/bin/bash ./src/bin/start.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garronej/chan-dongle-extended.git"
  },
  "bin": {
    "dongle": "./dist/bin/cli.js",
    "dongle-extended-admin": "./dist/bin/scripts.js"
  },
  "keywords": [
    "chan_dongle",
    "asterisk",
    "multipart SMS",
    "phonebook",
    "contact",
    "PIN"
  ],
  "author": "Joseph Garrone",
  "license": "MIT",
  "dependencies": {
    "chan-dongle-extended-client": "garronej/chan-dongle-extended-client",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "debug": "^2.6.1",
    "gsm-modem-connection": "garronej/gsm-modem-connection",
    "ini-extended": "github:garronej/ini-extended",
    "js-base64": "^2.1.9",
    "node-persist": "^2.0.10",
    "rejection-tracker": "github:garronej/rejection-tracker",
    "timer-extended": "garronej/timer-extended",
    "trackable-map": "garronej/trackable-map",
    "ts-events-extended": "garronej/ts-events-extended",
    "ts-exec-queue": "garronej/ts-exec-queue",
    "ts-gsm-modem": "garronej/ts-gsm-modem",
    "ts-promisify": "garronej/ts-promisify"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/colors": "^1.1.1",
    "@types/debug": "0.0.29",
    "@types/js-base64": "^2.1.5",
    "@types/node": "^4.2.2",
    "@types/node-persist": "0.0.31",
    "typescript": "^2.2.2"
  }
}
