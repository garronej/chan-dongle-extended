{
  "name": "chan-dongle-extended",
  "version": "4.4.0",
  "description": "feature SMS multipart, phonebook memory access, PIN code management",
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "postinstall": "npm run remove-bold-color",
    "remove-bold-color": "sed -i '/bold: string;/d' ./node_modules/@types/colors/index.d.ts || true",
    "tsc": "node ./node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "tsc-w": "node ./node_modules/typescript/bin/tsc -w -p ./tsconfig.json",
    "start": "sudo ./working_directory/start.sh",
    "log": "tail -f -n 1000 ./working_directory/current.log",
    "tarball": "sudo node dist/bin/installer tarball"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garronej/chan-dongle-extended.git"
  },
  "author": "Joseph Garrone",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "chan-dongle-extended-client": "github:garronej/chan-dongle-extended-client",
    "colors": "^1.1.2",
    "commander": "^2.11.0",
    "debug": "^2.6.1",
    "ini-extended": "github:garronej/ini-extended",
    "node-persist": "^2.1.0",
    "npm": "^6.0.0",
    "rejection-tracker": "github:garronej/rejection-tracker",
    "run-exclusive": "github:garronej/run-exclusive",
    "scripting-tools": "github:garronej/scripting-tools",
    "trackable-map": "garronej/trackable-map",
    "transfer-tools": "garronej/transfer-tools",
    "ts-ami": "github:garronej/ts-ami",
    "ts-events-extended": "garronej/ts-events-extended",
    "ts-gsm-modem": "git+https://206e4c24cb9a1e4a8d42b7ac86e4a026b02cee42:x-oauth-basic@github.com/garronej/ts-gsm-modem.git",
    "ts-sip": "github:garronej/ts-sip",
    "winston": "^3.0.0-rc4"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/colors": "^1.1.3",
    "@types/debug": "0.0.29",
    "@types/node": "^8.9.1",
    "@types/node-persist": "0.0.31",
    "typescript": "2.7.1"
  }
}
