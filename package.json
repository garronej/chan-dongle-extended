  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/colors": "^1.1.1",
    "@types/debug": "0.0.29",
    "@types/node": "^4.2.2",
    "@types/node-persist": "0.0.31",
    "typescript": "^2.4.1"
  }
